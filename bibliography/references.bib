@Article{Manay2004,
  Title                    = {{Integral invariant signatures}},
  Author                   = {Manay, S and Hong, B and Yezzi, a and Soatto, S},
  Journal                  = {Computer Vision - ECCV 2004},
  Year                     = {2004},
  Pages                    = {87--99},
  Volume                   = {3024},

  Abstract                 = {For shapes represented as closed planar contours, we introduce a class$\backslash$nof functionals that are invariant with respect to the Euclidean and$\backslash$nsimilarity group, obtained by performing integral operations. While$\backslash$nsuch integral invariants enjoy some of the desirable properties of$\backslash$ntheir differential cousins, such as locality of computation (which$\backslash$nallows matching under occlusions) and uniqueness of representation$\backslash$n(in the limit), they are not as sensitive to noise in the data. We$\backslash$nexploit the integral invariants to define a unique signature, from$\backslash$nwhich the original shape can be reconstructed uniquely up to the$\backslash$nsymmetry group, and a notion of scale-space that allows analysis$\backslash$nat multiple levels of resolution. The invariant signature can be$\backslash$nused as a basis to define various notions of distance between shapes,$\backslash$nand we illustrate the potential of the integral invariant representation$\backslash$nfor shape matching on real and synthetic data.},
