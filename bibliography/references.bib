@InProceedings{Nguyen2012,
  Title                    = {Height Measurement for Humans in Motion Using a Camera: A Comparison of Different Methods},
  Author                   = {Nguyen, Ngoc Hung and Hartley, Richard},
  Booktitle                = {2012 International Conference on Digital Image Computing Techniques and Applications (DICTA)},
